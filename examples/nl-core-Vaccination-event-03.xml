   <!-- VaccinatieIntentie -->
   <reasonCode><!-- reasonCode:vaccinationIndication --></reasonCode>
   <!-- VaccinatieAanleiding -->
   <reasonCode><!-- reasonCode:vaccinationMotive --></reasonCode>
