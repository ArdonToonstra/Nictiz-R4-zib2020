    <name value="zib ContactPerson-v3.4(2020EN)" />
  </mapping>
  <mapping>
    <identity value="zib-contactinformation-v1.2-2020EN" />
    <uri value="https://zibs.nl/wiki/ContactInformation-v1.2(2020EN)" />
    <name value="zib ContactInformation-v1.2(2020EN)" />
